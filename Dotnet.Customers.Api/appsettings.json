{
  "AppSettings": {
    "CacheSettings": {
      "AbsoluteExpiration": 10,
      "SlidingExpiration": 4
    }
  },
  "FeatureManagement": {
    "customer": true // Feature flag set to on
  },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*"
  }