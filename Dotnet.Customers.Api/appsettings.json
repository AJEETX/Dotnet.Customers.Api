{
  "AppSettings": {
    "CacheSettings": {
      "AbsoluteExpiration": 10,
      "SlidingExpiration": 4
    }
  },
  "FeatureManagement": {
    "V1": {
      "CUSTOMER": true // Feature flag set to on
    },
    "V2": {
      "CUSTOMER": false // Feature flag set to on
    }
  },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*"
  }